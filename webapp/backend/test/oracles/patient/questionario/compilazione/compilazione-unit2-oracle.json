{
    "metadata": {
        "version": "1.0",
        "description": "Oracle for Compilazione Questionario Unit 2",
        "createdAt": "2025-12-10",
        "author": "Antigravity"
    },
    "testCases": [
        {
            "id": "TC_RF3_4",
            "description": "Esecuzione Recupero - UUID non trovato",
            "input": {
                "idQuestionario": "00000000-0000-0000-0000-000000000000"
            },
            "expectedBehavior": {
                "type": "exception",
                "message": "Questionario non trovato"
            },
            "mockData": {
                "isUuid": true,
                "questionarioDb": []
            }
        },
        {
            "id": "TC_RF3_5",
            "description": "Esecuzione Recupero - Tipologia non trovata (Input non UUID)",
            "input": {
                "idQuestionario": "TipoErrato"
            },
            "expectedBehavior": {
                "type": "exception",
                "message": "Tipologia non trovata"
            },
            "mockData": {
                "isUuid": false,
                "tipologiaDb": []
            }
        },
        {
            "id": "TC_RF3_6",
            "description": "Esecuzione Recupero - UUID valido ma Tipologia non trovata",
            "input": {
                "idQuestionario": "11111111-1111-1111-1111-111111111111"
            },
            "expectedBehavior": {
                "type": "exception",
                "message": "Tipologia non trovata"
            },
            "mockData": {
                "isUuid": true,
                "questionarioDb": [
                    {
                        "idQuestionario": "...",
                        "nomeTipologia": "TipoMissing"
                    }
                ],
                "tipologiaDb": []
            }
        },
        {
            "id": "TC_RF3_7",
            "description": "Esecuzione Recupero - Successo",
            "input": {
                "idQuestionario": "22222222-2222-2222-2222-222222222222"
            },
            "expectedBehavior": {
                "type": "success",
                "expectedResult": {
                    "idQuestionario": "22222222-2222-2222-2222-222222222222",
                    "nomeTipologia": "Umore",
                    "tempoSomministrazione": 5,
                    "domande": [
                        {
                            "id": "q1",
                            "testo": "Come stai?",
                            "tipo": "scala",
                            "scalaMin": 0,
                            "scalaMax": 4,
                            "opzioni": [
                                "Male",
                                "Bene"
                            ]
                        }
                    ]
                }
            },
            "mockData": {
                "isUuid": true,
                "questionarioDb": [
                    {
                        "idQuestionario": "...",
                        "nomeTipologia": "Umore"
                    }
                ],
                "tipologiaDb": [
                    {
                        "domande": "Come stai?",
                        "tempoSomministrazione": 5,
                        "punteggio": "0;1",
                        "campi": "Male;Bene"
                    }
                ]
            }
        }
    ]
}